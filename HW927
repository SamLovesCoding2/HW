package hw927;

import java.util.ArrayList;
import java.util.HashMap;
import java.util.Iterator;
import java.util.List;
import java.util.TreeMap;
import java.util.TreeSet;

/**
 *
 * @author lupeiyu
 */
public class HW927
{

    /**
     * @param args the command line arguments
     */
    public static TreeSet<String> strSet;
    public static int length;
    public static Iterator<String> i;
    public static HashMap wordCount;
    public static List<Integer> numList;
    public static void main(String[] args)
    {
        strSet = new TreeSet<String>();
        strSet.add("1");
        strSet.add("12");
        strSet.add("123");
        strSet.add("1234");
        length = getMax(strSet);
        System.out.println(length);
        
    }
    public static int getMax(TreeSet<String> strSet)
    {
        i = strSet.iterator();
        int l = 0;
        int max = 0;
        String currentString = "";
        String currentString1 = "";
        if(strSet.size() == 0)
        {
        }
        else
        {
            currentString1 = i.next();
            max = currentString1.length();
            while(i.hasNext())
            {
               currentString = i.next();
               l = currentString.length();
               if(l>max)
               {
                   max = l;
                   
               }
            }
        }
        return max;
    }
    
    public static boolean hasOdd(TreeSet<Integer> target)
    {
        boolean isOdd = false;
        Iterator<Integer> in = target.iterator();
        if(target.size() == 0)
        {
            return false;
        }
        else
        {
            while(in.hasNext())
            {
                int num = in.next();
                if(num%2 ==1)
                {
                    isOdd = true;
                    break;
                }
            }
        }
        return isOdd;
    }
    public static int appearMost(List list)
    {
        
        wordCount = new HashMap<String ,Integer>();
        if(list.size() == 0)
        {
            return 0;
        }else
        {
            for(int i = 0;i<list.size();i++)
            {
                int currentNum = (int) list.get(i);
                
            }
        }
    }
}
