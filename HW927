    public static void main(String[] args)
    {
        strSet = new TreeSet<String>();
        numList = new ArrayList<Integer>();
        strSet.add("1");
        strSet.add("12");
        strSet.add("123");
        strSet.add("1234");
        numList.add(1);
        numList.add(1);
        numList.add(14);
        numList.add(12);
        numList.add(13);
        
        length = getMax(strSet);
        System.out.println(length);
        int most = appearMost(numList);
        System.out.println(most);
